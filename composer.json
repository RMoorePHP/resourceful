{
    "name": "rmoore/routable",
    "description": "Generate routes for laravel models",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Moore",
            "email": "robert@sliske.uk"
        }
    ],
    "require": {
        "illuminate/support": "^6.0",
        "illuminate/routing": "^6.0"
    },
    "require-dev": {
        "illuminate/events": "^6.0",
        "illuminate/database": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "RMoore\\Routable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RMoore\\Routable\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
